pipeline{
  agent any
  environment {
    GOPATH = "${env.WORKSPACE}/"
  }
  tools {
    go 'go1.22.4'
  }
  stages{
    stage('Build'){
      steps{
        echo 'go1.22.4 build'
        sh 'go build -o jenkins-pipeline-test'
      }
    }
  }
  post{
    always{
      archiveArtifacts iartifacts:'jenkins-pipeline-test',fingerprint:true,onlyIfSuccessful:true
    }
  }
}
